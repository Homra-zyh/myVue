{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "baseUrl": "./",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["element-plus/global"],
  },
  "include": ["src/**/*"],
   "exclude": ["node_modules", "dist"]
}